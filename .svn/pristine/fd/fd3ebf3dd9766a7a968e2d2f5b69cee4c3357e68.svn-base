#import "MenuItem.h"

@implementation MenuItem


- (id)initwithDictionary:(NSDictionary*)dic{
    if (self == [super init]) {
        
         self.announcement = [[dic valueForKey:@"announcement"] integerValue];
         self.classified = [[dic valueForKey:@"classified"] integerValue];
         self.event = [[dic valueForKey:@"event"] integerValue];
         self.gallery = [[dic valueForKey:@"gallery"] integerValue];
         self.offerAndPromotion = [[dic valueForKey:@"offerAndPromotion"] integerValue];
        self.forums = [[dic valueForKey:@"forum"] integerValue];
        
    }
    return self;
}

@end
